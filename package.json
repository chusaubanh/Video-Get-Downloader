{
  "name": "video-get-downloader",
  "private": true,
  "version": "1.2.7",
  "description": "Video-Get-Downloader - Download videos from social media without watermark",
  "author": "ChuSauBanh",
  "license": "MIT",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder --publish never",
    "electron:build:win": "npm run build && electron-builder --win --publish never",
    "electron:build:mac": "npm run build && electron-builder --mac --publish never",
    "electron:build:linux": "npm run build && electron-builder --linux --publish never",
    "electron:build:all": "npm run build && electron-builder --win --mac --linux --publish never",
    "electron:preview": "npm run build && electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron-updater": "^6.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.32",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.chusaubanh.videogetdownloader",
    "productName": "Video-Get-Downloader",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "copyright": "Copyright Â© 2024 ChuSauBanh",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "!electron/yt-dlp.exe",
      "!electron/yt-dlp-core.exe",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "electron/yt-dlp-core.exe",
        "to": "yt-dlp-core.exe",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "signAndEditExecutable": false,
      "publisherName": "ChuSauBanh"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Video-Get-Downloader",
      "include": "build/installer.nsh",
      "deleteAppDataOnUninstall": false
    },
    "portable": {
      "artifactName": "${productName}-${version}-Portable-${os}.${ext}"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.icns",
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "darkModeSupport": true
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "title": "${productName} ${version}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icons",
      "category": "Utility",
      "maintainer": "ChuSauBanh",
      "synopsis": "Video downloader without watermark",
      "description": "Download videos from social media platforms without watermark"
    },
    "appImage": {
      "artifactName": "${productName}-${version}-${os}.${ext}"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "chusaubanh",
        "repo": "Video-Downloader"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chusaubanh/Video-Downloader.git"
  }
}